{
	"name": "exceljs",
	"version": "4.4.1",
	"description": "Excel Workbook Manager - Read and Write xlsx and csv Files.",
	"private": false,
	"license": "MIT",
	"author": {
		"name": "Guyon Roche",
		"email": "guyon@live.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/exceljs/exceljs.git"
	},
	"engines": {
		"node": ">=8.3.0"
	},
	"main": "./excel.js",
	"browser": "./dist/exceljs.min.js",
	"types": "./index.d.ts",
	"files": [
		"dist",
		"lib",
		"excel.js",
		"LICENSE",
		"README.md",
		"README_zh.md",
		"index.ts",
		"index.d.ts"
	],
	"scripts": {
		"clean": "rm -fr dist && mkdir dist",
		"build:grunt": "grunt build",
		"build:esnext": "ngc -p tsconfig.esnext.json",
		"build": "npm run build:grunt && npm run build:esnext",
		"prepublishOnly": "npm run build",
		"publish": "npm publish ./dist"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier-eslint --write",
			"eslint --format node_modules/eslint-friendly-formatter",
			"git add"
		]
	},
	"keywords": [
		"xlsx",
		"json",
		"csv",
		"excel",
		"font",
		"border",
		"fill",
		"number",
		"format",
		"number format",
		"alignment",
		"office",
		"spreadsheet",
		"workbook",
		"defined names",
		"data validations",
		"rich text",
		"in-cell format",
		"outlineLevel",
		"views",
		"frozen",
		"split",
		"pageSetup"
	],
	"dependencies": {
		"archiver": "5.3.2",
		"dayjs": "1.11.13",
		"fast-csv": "4.3.6",
		"jszip": "3.10.1",
		"readable-stream": "3.6.2",
		"saxes": "5.0.1",
		"tmp": "0.2.3",
		"unzipper": "0.12.3",
		"uuid": "10.0.0"
	},
	"devDependencies": {
		"@angular/cli": "^9.0.0",
		"@angular/compiler": "^9.0.0",
		"@angular/compiler-cli": "^9.0.0",
		"@angular/core": "^9.0.0",
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.4",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-classes": "^7.23.8",
		"@babel/plugin-transform-modules-umd": "^7.1.0",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-react": "^7.23.3",
		"@babel/preset-typescript": "^7.23.3",
		"@rollup/plugin-babel": "^6.0.4",
		"@types/chai": "^4.2.12",
		"@types/mocha": "^8.0.3",
		"@types/node": "15.6.1",
		"babel-plugin-add-module-exports": "^1.0.4",
		"babel-preset-minify": "^0.4.3",
		"babelify": "^10.0.0",
		"browserify": "^16.5.2",
		"chai": "^4.2.0",
		"chai-datetime": "^1.7.0",
		"chai-xml": "^0.3.2",
		"core-js": "^3.6.5",
		"dirty-chai": "^2.0.1",
		"eslint": "^6.5.1",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-friendly-formatter": "^4.0.1",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-node": "^11.1.0",
		"express": "^4.16.4",
		"got": "^9.0.0",
		"grunt": "^1.3.0",
		"grunt-babel": "^8.0.0",
		"grunt-browserify": "^5.3.0",
		"grunt-contrib-copy": "^1.0.0",
		"grunt-contrib-jasmine": "^2.2.0",
		"grunt-contrib-watch": "^1.1.0",
		"grunt-exorcise": "^2.1.1",
		"grunt-rollup": "^12.0.0",
		"grunt-terser": "^2.0.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.2.13",
		"mocha": "^7.2.0",
		"prettier": "3.2.5",
		"prettier-eslint": "^11.0.0",
		"prettier-eslint-cli": "^5.0.0",
		"regenerator-runtime": "^0.13.7",
		"sax": "^1.2.4",
		"ts-node": "^8.10.2",
		"typescript": "^3.8.0",
		"zone.js": "~0.10.3"
	}
}
